{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            2,
            "double",
            {
                "avoidEscape": true
            }
        ],
        "no-param-reassign": [
            0
        ],
        "class-methods-use-this": [
            0
        ]
    }
}